<ResourceDictionary
	x:ClassModifier="internal"
	xmlns="https://github.com/avaloniaui"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:actipro="http://schemas.actiprosoftware.com/avaloniaui"
	>

	<DataTemplate x:Key="{x:Static actipro:BarsMvvmResourceKeys.RibbonControlGroupDefaultItemContainerTemplate}" DataType="actipro:RibbonControlGroupViewModel">
		<actipro:RibbonControlGroup
			HorizontalContentAlignment="{Binding HorizontalContentAlignment}"
			IsVisible="{Binding IsVisible}"
			ItemVariantBehavior="{Binding ItemVariantBehavior}"
			ItemsSource="{Binding Items}"
			SeparatorMode="{Binding SeparatorMode}"
			/>
	</DataTemplate>

</ResourceDictionary>