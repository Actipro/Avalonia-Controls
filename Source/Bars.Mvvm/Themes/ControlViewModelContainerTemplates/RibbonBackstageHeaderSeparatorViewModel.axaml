<ResourceDictionary
	x:ClassModifier="internal"
	xmlns="https://github.com/avaloniaui"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:actipro="http://schemas.actiprosoftware.com/avaloniaui"
	>

	<DataTemplate x:Key="{x:Static actipro:BarsMvvmResourceKeys.RibbonBackstageHeaderSeparatorDefaultItemContainerTemplate}" DataType="actipro:RibbonBackstageHeaderSeparatorViewModel">
		<actipro:RibbonBackstageHeaderSeparator
			HeaderAlignment="{Binding HeaderAlignment}"
			IsVisible="{Binding IsVisible}"
			Tag="{Binding Tag}"
			/>
	</DataTemplate>

</ResourceDictionary>