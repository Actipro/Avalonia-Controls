<ResourceDictionary
	x:ClassModifier="internal"
	xmlns="https://github.com/avaloniaui"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:actipro="http://schemas.actiprosoftware.com/avaloniaui"
	>

	<DataTemplate x:Key="{x:Static actipro:BarsMvvmResourceKeys.RibbonTabDefaultItemContainerTemplate}" DataType="actipro:RibbonTabViewModel">
		<actipro:RibbonTabItem
			ContextualTabGroupKey="{Binding ContextualTabGroupKey}"
			ControlVariants="{Binding ControlVariants}"
			GroupVariants="{Binding GroupVariants}"
			HeaderToolTip="{Binding Description}"
			IsEnabled="{Binding IsEnabled}"
			IsVisible="{Binding IsVisible}"
			ItemsSource="{Binding Groups}"
			Key="{Binding Key}"
			KeyTipText="{Binding KeyTipText}"
			Label="{Binding Label}"
			Tag="{Binding Tag}"
			Title="{Binding Title}"
			/>
	</DataTemplate>

</ResourceDictionary>