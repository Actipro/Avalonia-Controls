<ResourceDictionary
	x:ClassModifier="internal"
	xmlns="https://github.com/avaloniaui"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:actipro="http://schemas.actiprosoftware.com/avaloniaui"
	>

	<DataTemplate x:Key="{x:Static actipro:BarsMvvmResourceKeys.BarHeadingMenuItemItemContainerTemplate}" DataType="actipro:BarHeadingViewModel">
		<actipro:BarMenuHeading
			IsVisible="{Binding IsVisible}"
			Label="{Binding Label}"
			Tag="{Binding Tag}"
			/>
	</DataTemplate>

</ResourceDictionary>