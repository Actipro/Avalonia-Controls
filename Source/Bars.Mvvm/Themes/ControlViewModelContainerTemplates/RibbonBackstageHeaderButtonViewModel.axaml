<ResourceDictionary
	x:ClassModifier="internal"
	xmlns="https://github.com/avaloniaui"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:actipro="http://schemas.actiprosoftware.com/avaloniaui"
	>

	<DataTemplate x:Key="{x:Static actipro:BarsMvvmResourceKeys.RibbonBackstageHeaderButtonDefaultItemContainerTemplate}" DataType="actipro:RibbonBackstageHeaderButtonViewModel">
		<actipro:RibbonBackstageHeaderButton
			Command="{Binding Command}"
			CommandParameter="{Binding CommandParameter}"
			HeaderAlignment="{Binding HeaderAlignment}"
			IsVisible="{Binding IsVisible}"
			Key="{Binding Key}"
			KeyTipText="{Binding KeyTipText}"
			Label="{Binding Label}"
			SmallIcon="{Binding SmallIcon}"
			Tag="{Binding Tag}"
			Title="{Binding Title}"
			ToolTip.Tip="{Binding Description}"
			/>
	</DataTemplate>

</ResourceDictionary>